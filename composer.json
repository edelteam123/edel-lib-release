{
    "name": "edelweiss/api-connect-php",
    "version": "1.0.0",
    "description": "APIs to trade from Edelweiss.",
    "keywords": [
        "Open API",
        "Trade",
        "Edelweiss",
        "Edelweiss PHP Library"
      ],
    "homepage": "https://www.edelweiss.in",
    "license": "MIT",
    "authors": [{
        "name": "Edelweiss",
        "homepage": "https://www.edelweiss.in",
        "email": "helpdesk@edelweiss.in"
    }],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "apache/log4php": "2.3.0",
        "rakit/validation": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "com\\edel\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "com\\edel\\Test\\": "test/"
        }
    }
}
